<?xml version="1.0"?>
<robot name="rae" xmlns:xacro="http://ros.org/wiki/xacro">

    <link name="base_frame"/>
    <link name="chassis">
      <inertial>
        <mass value="1.0"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <inertia
          ixx = "0.166667"
          ixy = "0"
          ixz = "0"
          iyy = "0.166667"
          iyz = "0"
          izz = "0.166667"
        />
      </inertial>

      <collision>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
          <mesh filename="package://depthai_bridge/urdf/models/rae_chassis.stl" scale="1.0 1.0 1.0"/>
        </geometry>
      </collision>

      <visual>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
          <mesh filename="package://depthai_bridge/urdf/models/rae_chassis.stl" scale="1.0 1.0 1.0"/>
        </geometry>
        <material name="mat">
          <lighting>1</lighting>
          <!-- <color rgba="0.1 0.1 0.1 1.0"/> -->
          <shader type='pixel'/>
          <emissive>0 0 0 1</emissive>
        </material>
        <transparency>0</transparency>
        <cast_shadows>1</cast_shadows>
      
      </visual>
    </link>

    <joint name="base_chassis_joint" type="fixed">
        <parent link="base_frame"/>
        <child link="chassis"/>
        <origin rpy="1.57 0.0 1.57" xyz="0.0 0.0 0.0" />
    </joint>

    <link name="left_wheel">
      <inertial>
        <mass value="1.0"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <inertia
          ixx = "0.166667"
          ixy = "0"
          ixz = "0"
          iyy = "0.166667"
          iyz = "0"
          izz = "0.166667"
        />
      </inertial>

      <collision>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
            <mesh filename="package://depthai_bridge/urdf/models/rae_wheel.stl" scale="1.0 1.0 1.0"/>
        </geometry>
      </collision>

      <visual>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
            <mesh filename="package://depthai_bridge/urdf/models/rae_wheel.stl" scale="1.0 1.0 1.0"/>
        </geometry>
      </visual>
    </link>

    <link name="right_wheel">
      <inertial>
        <mass value="1.0"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <inertia
          ixx = "0.166667"
          ixy = "0"
          ixz = "0"
          iyy = "0.166667"
          iyz = "0"
          izz = "0.166667"
        />
      </inertial>

      <collision>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
            <mesh filename="package://depthai_bridge/urdf/models/rae_wheel.stl" scale="1.0 1.0 1.0"/>
        </geometry>
      </collision>

      <visual>
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        <geometry>
            <mesh filename="package://depthai_bridge/urdf/models/rae_wheel.stl" scale="1.0 1.0 1.0"/>
        </geometry>
      </visual>
    </link>

    <joint name="base_joint" type="fixed">
        <parent link="base_frame"/>
        <child link="chassis"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

    <joint name='left_wheel_hinge' type='revolute'>
      <origin rpy="0.0 0.0 0.0 " xyz="-0.0497 0.00518 -0.03048"/>
      <child  link="left_wheel"/>
      <parent link="chassis"/>
      <axis rpy="0 0 0" xyz="1 0 0"/>
      <limit lower="-1e+16" upper="1e+16" effort="100" velocity="100"/>
    </joint>

    <joint name='right_wheel_hinge' type='revolute'>
      <origin rpy="0.0 3.14159 0.0 " xyz="0.0497 0.00518 -0.03048"/>
      <child  link="right_wheel"/>
      <parent link="chassis"/>
      <axis rpy="0 0 0" xyz="1 0 0"/>
      <limit lower="-1e+16" upper="1e+16" effort="100" velocity="100"/>
    </joint>

